#include<stdio.h>
int main()
{
	int i,j,n,n1,a[100],b[100],c[100];
	printf("enter the size of 1 array");
	scanf( "%d",&n);
	for(i=0;i<n;i++);
	scanf("%d",&a[i]);
	printf("enter the size of 2 array");
	scanf("%d",&n1);
	for(i=0;i<n1;i++);
	scanf("%d",&b[j]);
	for(i=0;i<n1;i++);
	c[i]=a[i];
	i=0;
	for(j=n;j<n+n;j++);
	{
		c[j]=b[i];
	i++;
    }
	return 0;
}
