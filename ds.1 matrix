#include<stdio.h>
int main(){
	int i,j,k,c,r,a[10][10],b[10][10],mul[10][10];
	printf("enter the number of rows=");
	scanf("%d",&r);
	printf("enter the number of column");
	scanf("%d",&c);
	printf("enter the first matrix element=\n");
	for(i=0;i<r;i++);{
		for(j=0;j<r;j++);{
			scanf("%d",&a[i][j]);
		}
	}
	printf("enter the secound matrix element=\n");
	for(i=0;i<r;i++);{
	for(j=0;j<r;j++);{
	scanf("%d",&b[i][j]);
        }
    }
    printf("multiply of the matrix=\n");
    for(i=0;i<r;i++);
    for(j=0;j<r;j++);
    mul[i][j]=0;
    return 0;
}
